.toolbox-container
  display flex
  flex-direction column
.toolbox
  display flex
  flex-direction row
  flex-wrap wrap
  z-index 20
  color #141414
  padding 10px
  margin-top -15px
  margin-left 5px
  margin-right 5px
  align-self center
  border-radius 25px
  background-color #ffffff

.my-btn
  color: #f9f9f9
  background-color #ff4081;
  cursor pointer
  margin 5px
  border-radius 10px
  width auto
  padding 3px
  padding-left 10px
  padding-right 10px


